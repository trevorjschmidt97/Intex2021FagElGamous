                        <option value="150">150</option>
                        <option value="160">160</option>
                        <option value="170">170</option>
                        <option value="180">180</option>
                        <option value="190">190</option>
                        <option value="200">200</option>
                    </select>

                </div>
                <div class="select-box">

                    <label asp-for="Quadrant" class="label select-box1"><span class="label-desc">Choose area</span> </label>
                    <select class="select" asp-for="Quadrant">
                        <option value="SE">SE</option>
                        <option value="NE">NE</option>
                        <option value="SW">SW</option>
                        <option value="NW">NW</option>
                    </select>

                </div>

                <fieldset class="form-fieldset ui-input __first funkay">

                    <label>
                        <span>Burial Number</span>

                    </label>
                    <input type="text" tabindex="0" placeholder="1" />
                </fieldset>
                <fieldset class="form-fieldset ui-input __first funkay">

                    <label>
                        <span>South to Head</span>

                    </label>
                    <input type="text" tabindex="0" placeholder="measurement in meters" />
                </fieldset>
                <fieldset class="form-fieldset ui-input __first funkay">

                    <label>
                        <span>South to Feet</span>

                    </label>
                    <input type="text" tabindex="0" placeholder="measurement in meters" />
                </fieldset>
