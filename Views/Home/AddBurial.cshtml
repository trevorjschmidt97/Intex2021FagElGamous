                    East
                </label>

                <label>
                    <input required asp-for="EW" value="W" type="radio" />
                    <span class="radio-ghost funkay"></span>
                    West
                </label>
                <div class="select-box">
                    <label asp-for="Ewtop" class="label select-box1"><span class="label-desc">Choose East Top</span> </label>
                    <select class="select" required asp-for="Ewtop">
                        <option value="0">0</option>
                        <option value="10">10</option>
                        <option value="20">20</option>
                        <option value="30">30</option>
                        <option value="40">40</option>
